<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.single.flowlayout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LanuchActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".TextFlowActivity" />
        <activity android:name=".PhotoFlowActivity" />
        <activity android:name=".DiffHeightTextFlowActivity" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".SuspensionProductActivity" />
        <activity android:name=".ViewPagerActivity" />
        <activity android:name=".LongClickDeleteTextFlowActivity" />

    </application>

</manifest>